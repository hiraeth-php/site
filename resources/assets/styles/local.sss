:root
    --slate: rgb(85, 92, 97)
    --air: rgb(75, 109, 132)

    --container-max-width: 1280px

    --logo: url(data:image/png;base64,)

h1
    font-size: 3rem

h2
    margin-top: 2rem
    font-size: 2.6rem

body
    & > header,
    & > footer,
    & > section
        @mixin container

    & > header
        & > h2
            @mixin -center
            @mixin -align-center

            margin-top: 0

        & > .logo
            @mixin -center
            @mixin -grow
            @mixin -cover

            --width: 325px
            --height: 200px


    & > section.layout
        @mixin grid

        & > nav
            @mixin -keep-3

        & > main
            @mixin -keep-9


.action
    @mixin button
    @mixin -pad
    @mixin -outline
    @mixin -round

    text-decoration: none

.callouts
    @mixin container
    @mixin -align-center
    @mixin -fill

    --bg-color: var(--slate)

    & .action
        @mixin -color

        --color: var(--white)


.intro
    @mixin container
